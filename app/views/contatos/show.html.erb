<section class="primary-bg">
  	<div class="container">
    	<div class="row">
      		<div class="col-md-12">
		        <div class="section_heading">
          			<h2> Detalhes Contato</h2>
        		</div>
        		<p id="notice"><%= notice %></p>
        		<div class="form-group">
        			<label>Nome</label>
        			<input type="text" class="form-control" value="<%= @contato.nome %>" readonly />
  				</div>
  				<div class="form-group">
  					<label>Email</label>
        			<input type="text" class="form-control" value="<%= @contato.email %>" readonly />
  				</div>    
  				<div class="section_btn">
        			<%= link_to 'Novo Contato', new_contato_path, :class=>'btn btn-default' %>
        			<%= link_to 'Editar', edit_contato_path(@contato) , :class =>'btn btn-default' %> 
        			<%= link_to 'Voltar', contatos_path , :class =>'btn btn-default' %> 
        		</div>
       		</div>
      	</div>
    </div>
</section>